# HookHunt Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Spotify Integration
VITE_SPOTIFY_CLIENT_ID=your_spotify_client_id_here
VITE_SPOTIFY_REDIRECT_URI=http://localhost:5173/games/hookhunt/callback

# Development Settings (optional)
VITE_DEBUG_MODE=false
VITE_HOOK_DETECTION_LOG=false

# Production Settings (set for production builds)
# VITE_SPOTIFY_REDIRECT_URI=https://yourdomain.com/games/hookhunt/callback